N150 DEFINE SOE_KOLLI_AB        AS $A_OUTSE[2]  ;DB18.DBX46.1 - Lader im Stoerbereich Auslauf
N160 DEFINE SOE_FREI_EB         AS $A_OUTSE[3]  ;DB18.DBX46.2 - Lader außer Stoerbereich Einlauf
N170 DEFINE SOE_EXTBR_Z2_LOESEN AS $A_OUTSE[4]  ;DB18.DBX46.3 - Externe bremse Z2-Achse loesen
