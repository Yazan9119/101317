N110 DEFINE SIE_ZUSTIMM         AS $A_INSE[2]  ;DB18.DBX38.1 - Zustimmbetrieb aktiv
N120 DEFINE SIE_SE_LP_IO        AS $A_INSE[3]  ;DB18.DBX38.2 - Schutzkreis Ladeportal i.O.
N130 DEFINE SIE_L_LOCK          AS $A_INSE[4]  ;DB18.DBX38.3 - Laufwagensperre vorne
;N00 DEFINE SIE_SE_EB_IO        AS $A_INSE[4]  ;DB18.DBX38.3 - Schutzkreis Einlauf i.O.
;N00 DEFINE SIE_SE_AB_IO        AS $A_INSE[5]  ;DB18.DBX38.4 - Schutzkreis Auslauf i.O.
;
;>>> Ausgaenge
